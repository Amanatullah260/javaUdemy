package com.in28minutes.firstjavaproject;

public class MultiplicationTable2 {
	void print() {
	for (int i = 1; i<=10; i++) {
		System.out.printf("%d * %d = %d", 5, i, 5 * i).println();
		}
	}
	void print(int table2) {
		for (int i = 1; i<=10; i++) {
			System.out.printf("%d * %d = %d", table2, i, table2 * i).println();
			}
		}
	void print(int table2, int from, int to) {
		for (int i = from; i<=to; i++) {
			System.out.printf("%d * %d = %d", table2, i, table2 * i).println();
			}
		}
}


package com.in28minutes.firstjavaproject;

public class MultiplicationTable2Runner {

	public static void main(String[] args) {
		MultiplicationTable2 table2 = new MultiplicationTable2();
		//table2.print();
		table2.print(6);
		table2.print(6, 11,20);
	}
}

output
6 * 16 = 96
6 * 17 = 102
6 * 18 = 108
6 * 19 = 114
6 * 20 = 120

package com.in28minutes.oops;

public class MotorBikeRunner {

	public static void main(String[] args) {
		MotorBike ducati = new MotorBike();
		MotorBike honda = new MotorBike();

		ducati.start();
		honda.start();

	}

}

package com.in28minutes.oops;

public class MotorBike {
	
void start() {
		System.out.println("Bike Started");
	}
}

output
Bike Started
Bike Started
package com.in28minutes.oops;

public class MotorBikeRunner {

	public static void main(String[] args) {
		MotorBike ducati = new MotorBike();
		MotorBike honda = new MotorBike();

		ducati.start();
		honda.start();

		ducati.setSpeed(100);
		System.out.println(ducati.getSpeed());

		honda.setSpeed(80);
		System.out.println(honda.getSpeed());

	}

}


package com.in28minutes.oops;

public class MotorBike {
	// state
	private int speed;

	// behaviour
	// method
	// inputs - int speed
	// outputs - void
	// name setSpeed
	void setSpeed(int speed) { // local variable
		this.speed = speed;
	}

	int getSpeed() {
		return this.speed;
	}

	void start() {
		System.out.println("Bike Started");
	}
}

output

Bike Started
Bike Started
100
80


